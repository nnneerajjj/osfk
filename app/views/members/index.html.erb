<% content_for :title, @page.title %>
<% content_for :header, @page.title %>
<% content_for :description, @page.description %>

  <div class="inner-content page">
      <h1><%= @page.header %></h1>
      <p>
        <ul>
          <% @users.each do |user| %>
            <li class="member">
              <div class="round-icon"><%= fix_number(user.number) %></div>
              <div>
                <h5><%= user.name %></h5>
              </div>
              <div style="clear: both"></div>
            </li>
          <% end %>
        </ul>
      </p>
  </div>
