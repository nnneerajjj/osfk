<div class="inner-content page">
  <h1><%= I18n.t(:settings) %></h1>

  <div class="eight column">
    <%= form_for(resource, as: resource_name, url: edit_registration_path(resource_name)) do |f| %>
      <div>

        <%= f.label :firstname %>
