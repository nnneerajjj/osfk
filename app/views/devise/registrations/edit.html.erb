        <%= f.label :lastname %>
        <%= f.text_field :lastname %>

        <%= f.label :identity_number %> <%= t :'identity_number_hint' %>
        <%= f.text_field :identity_number %>

        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.label :telephone %>
        <%= f.text_field :telephone %>
w
        <%= f.label :address %>
