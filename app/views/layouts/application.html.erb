  <div class="outter-wrapper header-area header-style-4">

    <div class="wrapper clearfix logo-container with-menu">

      <div id="mobile-header" class="">
        <a id="responsive-menu-button" href="#sidr-main">
          <em class="fa fa-bars"></em> <%= t :menu %>
        </a>
      </div>

      <header>

          <div id="navigation" class="clearfix">

          <ul class="menuHideBtn">
            <li><a id="closebtn" class="fa" href="#">&#xf00d;</a></li>
          </ul>

          <div class="main-header-left left adjust-left">

            <a class="logo" href="<%= root_path %>">
              <img src="<%= image_path('osfk.png') %>" alt="<%= t :heading %>">
            </a>

          </div>

          <%= render 'shared/menu' %>
        </div>
      </header>
    </div>
  </div>

  <div class="outter-wrapper breadcrumb-wrapper">
    <div class="wrapper">
      <%= link_to '', root_path, class: 'fa' %>
      <%= link_to t(:heading), root_path %>
      <%= yield :breadcrumb %>
    </div>
  </div>

  <div class="message success<%= flash[:notice].blank? ? ' hide' : '' %>">
    <em class="fa"></em> <span><%= flash[:notice] %></span>
  </div>


  <div class="message error<%= flash[:error].blank? ? ' hide' : '' %>">
    <em class="fa"></em> <span><%= flash[:error] %></span>
  </div>

  <div class="message notice<%= flash[:alert].blank? ? ' hide' : '' %>">
    <em class="fa"></em> <span><%= flash[:alert] %></span>
  </div>

  <% banner = yield :banner %>
  <% if banner.present? %>
    <%= banner %>
  <% else %>

  <% end %>

  <div class="outter-wrapper body-wrapper">
    <div class="wrapper ad-pad clearfix">
      <% content = yield :contents %>
      <% if content.present? %>
        <%= content %>
      <% else %>
        <div class="col-3-4">
          <%= yield %>
        </div>

        <aside class="col-1-4 last centered">
