          <div class="ad"><a href="http://www.fiskelankar.se/"><img alt="" src="http://www.sportnik.com/file/show/1660555"></a></div>
          <div class="ad"><a href="http://www.mcdonalds.se/SWEDEN/restsite.nsf/Search/C2236409C4802190C125686400487402"><img alt="" src="http://www.sportnik.com/file/show/1728103"></a></div>
          <div class="ad"><a href="http://www.hi-sport.se/sidor/b_akersberga.html"><img alt="" src="http://www.sportnik.com/file/show/1728134"></a></div>
          <div class="ad"><a href="http://www.domarudden.se/"><img alt="" src="http://www.sportnik.com/file/show/1660516"></a></div>
          <div class="ad"><a href="http://www.flugspon.se/"><img alt="" src="http://www.sportnik.com/file/show/1965433"></a></div>
        </aside>

        <% aside = yield :aside %>
        <% if aside.present? %>
          <%= aside %>
        <% end %>
      <% end %>
    </div>
  </div>

  <%= render 'layouts/footer' %>

